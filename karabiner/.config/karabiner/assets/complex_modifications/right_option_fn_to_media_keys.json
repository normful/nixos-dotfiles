{
  "title": "Control + Function Keys to Media Control",
  "rules": [
    {
      "description": "right_option + f1 to display_brightness_decrement",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": [
                "right_option"
              ]
            }
          },
          "to": [
            {
              "key_code": "display_brightness_decrement"
            }
          ]
        }
      ]
    },
    {
      "description": "right_option + f2 to display_brightness_increment",

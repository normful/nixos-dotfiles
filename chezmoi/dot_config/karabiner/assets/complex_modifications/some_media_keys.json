{
  "title": "Right Option + Right Shift + some Function Keys to some Media Keys",
  "rules": [
    {
      "description": "right_option + right_shift + f1: display_brightness_decrement",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f1",
            "modifiers": {
              "mandatory": ["right_option", "right_shift"]
            }
          },
          "to": [
            {
              "key_code": "display_brightness_decrement"
            }
          ]
        }
      ]
    },
    {
      "description": "right_option + right_shift + f2: display_brightness_increment",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f2",
            "modifiers": {
              "mandatory": ["right_option", "right_shift"]
            }
          },
          "to": [
            {
              "key_code": "display_brightness_increment"
            }
          ]
        }
      ]
    },
    {
      "description": "right_option + right_shift + f10: mute",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f10",
            "modifiers": {
              "mandatory": ["right_option", "right_shift"]
            }
          },
          "to": [
            {
              "key_code": "mute"
            }
          ]
        }
      ]
    },
    {
      "description": "right_option + right_shift + f11: volume_decrement",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f11",
            "modifiers": {
              "mandatory": ["right_option", "right_shift"]
            }
          },
          "to": [
            {
              "key_code": "volume_decrement"
            }
          ]
        }
      ]
    },
    {
      "description": "right_option + right_shift + f12: volume_increment",
      "manipulators": [
        {
          "type": "basic",
          "from": {
            "key_code": "f12",
            "modifiers": {
              "mandatory": ["right_option", "right_shift"]
            }
          },
          "to": [
            {
              "key_code": "volume_increment"
            }
          ]
        }
      ]
    }
  ]
}
